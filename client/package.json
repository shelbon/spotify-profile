{
  "name": "spotify-profile-client",
  "version": "1.0.0",
  "private": true,
  "description": "spotify profile client",
  "scripts": {
    "build": "routify -b && SET NODE_ENV=production && rollup -c",
    "dev": "run-p dev:*",
    "dev:routify": "routify",
    "dev:rollup": "nollup -c  --verbose",
    "start-http2": " sirv public --no-clear --cors --http2 --key localhost-key.pem --cert localhost-cert.pem",
    "start": "sirv public --single --no-clear --cors"
  },
  "devDependencies": {
    "@iconify/icons-bx": "^1.1.0",
    "@iconify/svelte": "^1.0.4",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@roxi/routify": "^2.18.0",
    "axios": "^0.21.1",
    "dotenv": "^9.0.1",
    "esbuild": "^0.11.15",
    "fs-extra": "^10.0.0",
    "nollup": "^0.16.4",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-esbuild": "^4.0.1",
    "rollup-plugin-hot": "^0.1.1",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-svelte-hot": "^1.0.0-7",
    "svelte": "^3.0.0"
  },
  "dependencies": {
    "@sveltestack/svelte-query": "^1.4.0",
    "sirv-cli": "^1.0.0"
  }
}
