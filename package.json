{
  "name": "spotify-profile-app",
  "version": "0.0.0",
  "scripts": {
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "prepare": "npm run vite-workaround",
    "vite-workaround": "node _helper/vite-workaround.js",
    "dev": "run-p dev:*",
    "dev:routify": "routify",
    "dev:vite": "vite --host 0.0.0.0",
    "build": "routify -b && vite build ",
    "serve": "vite preview"
  },
  "type": "module",
  "devDependencies": {
    "@babel/preset-env": "7.22.9",
    "@iconify-icons/bx": "1.2.6",
    "@iconify/svelte": "3.1.4",
    "@originjs/vite-plugin-commonjs": "1.0.3",
    "@roxi/routify": "2.18.12",
    "@smui/card": "6.1.4",
    "@sveltejs/vite-plugin-svelte": "2.4.4",
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/svelte": "4.0.3",
    "autoprefixer": "10.4.14",
    "babel-jest": "29.6.2",
    "jest": "29.6.2",
    "jest-transform-svelte": "2.1.1",
    "npm-run-all": "4.1.5",
    "svelte": "4.1.2",
    "svelte-loading-spinners": "0.3.4",
    "vite": "4.4.8"
  },
  "dependencies": {
    "@sveltestack/svelte-query": "1.6.0",
    "luxon": "3.3.0",
    "svelte-chartjs": "1.1.4"
  },
  "browserslist": [
    "defaults"
  ],
  "postcss": {
    "map": false,
    "plugins": {
      "autoprefixer": true
    }
  }
}
