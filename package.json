{
  "name": "spotify-profile-app",
  "version": "0.0.0",
  "scripts": {
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "prepare": "npm run vite-workaround",
    "vite-workaround": "node _helper/vite-workaround.js",
    "dev": "run-p dev:*",
    "dev:routify": "routify",
    "dev:vite": "vite --host 0.0.0.0",
    "build": "routify -b && vite build ",
    "serve": "vite preview"
  },
  "type": "module",
  "devDependencies": {
    "@babel/preset-env": "7.15.0",
    "@iconify-icons/bx": "1.1.2",
    "@iconify/svelte": "2.0.0",
    "@originjs/vite-plugin-commonjs": "1.0.0-beta7",
    "@roxi/routify": "2.18.3",
    "@smui/card": "4.2.0",
    "@sveltejs/vite-plugin-svelte": "1.0.0-next.19",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/svelte": "3.0.3",
    "autoprefixer": "10.3.1",
    "babel-jest": "27.0.6",
    "jest": "27.0.6",
    "jest-transform-svelte": "2.1.1",
    "npm-run-all": "4.1.5",
    "svelte": "3.42.2",
    "svelte-loading-spinners": "0.1.7",
    "vite": "2.5.0"
  },
  "dependencies": {
    "@sveltestack/svelte-query": "1.4.1",
    "luxon": "^2.0.2",
    "svelte-chartjs": "1.0.1"
  },
  "browserslist": [
    "defaults"
  ],
  "postcss": {
    "map": false,
    "plugins": {
      "autoprefixer": true
    }
  }
}
